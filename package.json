{
  "name": "rocket-ui-test",
  "version": "0.0.1",
  "description": "Test for UI Engineering Candidates",
  "main": "''",
  "scripts": {
    "build": "npx parcel build src/template.html",
    "start": "npx parcel serve src/template.html -p 7357",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "test": "export NODE_ENV=test && jest",
    "pretty": "prettier --print-width 100 --single-quote all --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ReactJSResources/react-webpack-babel"
  },
  "author": "ReactJSResources",
  "license": "MIT",
  "homepage": "https://github.com/ReactJSResources/react-webpack-babel#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "6.26.0",
    "normalize.css": "^7.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "styles": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^22.0.4",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "^22.0.4",
    "loglevel": "^1.6.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.2.0"
  },
  "browserslist": [
    "Android 2.3",
    "Android >= 4",
    "Chrome >= 35",
    "Firefox >= 31",
    "Explorer >= 9",
    "iOS >= 7",
    "Opera >= 12",
    "Safari >= 7.1"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
